  "scripts": {
    "build": "yarn lint && NODE_ENV=production parcel build src/index.html --no-cache",
    "dev": "yarn clean && parcel src/index.html -p 3000",
    "lint": "tsc --noEmit --skipLibCheck && eslint 'src/**/*.{js,ts,tsx}' --max-warnings=0",
